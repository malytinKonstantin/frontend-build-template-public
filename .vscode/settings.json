{
  "editor.tabSize": 2,
  "prettier.tabWidth": 2,
  "editor.fontSize": 12,
  "editor.insertSpaces": true,
  "window.zoomLevel": 0,
  "editor.detectIndentation": false,
  "editor.formatOnSave": true,
  "eslint.autoFixOnSave": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    {
      "language": "typescript",
      "autoFix": true
    },
    {
      "language": "typescriptreact",
      "autoFix": true
    }
  ]
}
