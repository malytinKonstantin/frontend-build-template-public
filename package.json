{
  "name": "frontend-build-template",
  "version": "1.0.0",
  "description": "",
  "main": "index.tsx",
  "config": {},
  "scripts": {
    "//b": "--------------- BUILD ---------------",
    "start": "set NODE_ENV=development&& webpack-dev-server --open --config config/webpack/dev.js --content-base ./ --hot --inline",
    "build:prod": "set NODE_ENV=production&& webpack --config config/webpack/prod.js --mode production",
    "build:dev": "set NODE_ENV=development&& webpack --config config/webpack/dev.js --mode development",
    "build": "npm run build:prod && npm run storybook:build && npm run test:coverage",
    "//a": "--------------- BUNDLE_ANALYZE ---------------",
    "generate-bundle-stats:prod": "webpack --config config/webpack/prod.js --mode production --profile --json > bundle-stats.json",
    "generate-bundle-stats:dev": "webpack --config config/webpack/dev.js --mode development --profile --json > bundle-stats.json",
    "analize:prod": "npm run generate-bundle-stats:prod && npm run analize",
    "analize:dev": "npm run generate-bundle-stats:dev && npm run analize",
    "analize": "webpack-bundle-analyzer bundle-stats.json",
    "//t": "--------------- TESTS ---------------",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json",
    "test": "set NODE_ENV=test&& jest",
    "test:watch": "set NODE_ENV=test&& jest --watch",
    "test:coverage": "set NODE_ENV=test&& jest --coverage",
    "//s": "--------------- STORYBOOK ---------------",
    "storybook": "npm run test:generate-output && start-storybook -c config/.storybook -p 6006",
    "storybook:build": "build-storybook -c config/.storybook -o dist/lib",
    "storybook:build:debug": "build-storybook -c config/.storybook -o dist/lib --debug-webpack",
    "//l": "--------------- LINT ---------------",
    "prettier": "prettier --write 'src/**/*.{ts,tsx}'",
    "stylelint": "stylelint \"src/**/*.scss\" --fix",
    "eslint": "eslint \"src/**/*.{ts,tsx}\" --fix --max-warnings=0",
    "eslint:debug": "eslint --debug \"src/**/*.{ts,tsx}\"",
    "fix-code": "npm run prettier && npm run stylelint && npm run eslint",
    "//c": "--------------- CODEGEN ---------------",
    "codegen": "graphql-codegen --config codegen.yml",
    "codegen:watch": "graphql-codegen --config codegen.yml --watch",
    "//d": "--------------- DEPLOY ---------------",
    "predeploy": "npm run build:prod",
    "deploy": "gh-pages -d dist"
  },
